/*******************************
         Site Overrides
*******************************/
.ui.table {
  border-collapse: separate;
  border-spacing:0 5px;
}
.ui.table td {
  background: #DCDDDD;
}
.ui.table tr {
  padding-bottom: 1em;
}
.ui.table tr td:first-child {
  border-top-left-radius: 10px;
  border-bottom-left-radius: 10px;
}
